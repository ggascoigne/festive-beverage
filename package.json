{
  "name": "festive-beverage",
  "version": "0.1.0",
  "description": "FestiveBeverage site",
  "author": "guy@wyrdrune.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ggascoigne/festive-beverage.git"
  },
  "files": [],
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*.js",
      "./src/*.jsx",
      "./src/*/index.ts",
      "./src/*/index.tsx",
      "./src/*/index.js",
      "./src/*/index.jsx"
    ]
  },
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "boot": "pnpm db:migrate:up && pnpm prepareGraphql && pnpm codegen && pnpm gitHash",
    "build": "pnpm boot && NODE_ENV=production next build",
    "codegen": "graphql-codegen --config codegen.ts",
    "db:clean": "pnpm db:start && ./scripts/createCleanDb.ts",
    "db:import": "pnpm db:start && ./scripts/toLocal.sh",
    "db:migrate:generate": "knex migrate:make --knexfile ./support/knexfile.jms",
    "db:migrate:up": "knex migrate:latest --knexfile ./support/knexfile.mjs",
    "db:setup": "pnpm db:clean && pnpm db:migrate:up",
    "db:start": "docker compose up -d && ./scripts/wait-for-postgres.sh -c festive-beverage-postgres",
    "dev": "pnpm boot && next dev -p 40000",
    "dev:debug": "pnpm boot && NODE_OPTIONS='--inspect' next dev -p 40000",
    "import": "pnpm db:clean && pnpm db:migrate:up && ./scripts/import.ts",
    "importPush": "pnpm run import && pnpm upload",
    "download": "./scripts/toLocal.sh",
    "upload": "./scripts/toAws.sh",
    "format": "prettier --no-color --loglevel warn --write '{src,api,scripts,types}/**/*.{[mc]?js,jsx,[mc]?ts,tsx,css,scss,json,graphql}'",
    "gitHash": "./scripts/getGitHash.sh",
    "lint": "eslint '{src,api,scripts,shared,support,types}/**/*.{[mc]js,[mc]ts,[jt]s,tsx}' ./.eslintrc.cjs",
    "preinstall": "npx only-allow pnpm",
    "prepareGraphql": "./scripts/prepareGraphql.ts",
    "disabled:postinstall": "./scripts/buildSql.sh",
    "start": "BROWSER=none next start -p 40000",
    "test": "next test"
  },
