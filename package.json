  },
  "resolutions": {
    "@swc/core": "1.2.82"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "semi": false,
    "jsxSingleQuote": true,
    "trailingComma": "es5"
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    },
    ".js, .jsx": {
      "style": "module"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "./scripts/noForbiddenCode.sh",
      "prettier --no-color --write",
      "eslint"
    ],
    "*.{css,scss,graphql}": [
      "prettier --no-color --write"
    ]
  },
  "browserslist": [
    ">0.5%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "pnpm": {
    "patchedDependencies": {
      "ts-node@10.9.1": "patches/ts-node@10.9.1.patch"
    }
  }
}