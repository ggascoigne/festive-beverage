{
  "name": "festive-beverage",
  "version": "0.1.0",
  "description": "FestiveBeverage site",
  "author": "guy@wyrdrune.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ggascoigne/festive-beverage.git"
  },
  "files": [],
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*.js",
      "./src/*.jsx",
      "./src/*/index.ts",
      "./src/*/index.tsx",
      "./src/*/index.js",
      "./src/*/index.jsx"
    ]
  },
  "scripts": {
    "boot": "pnpm db:migrate:up && pnpm prisma generate --sql && pnpm gitHash",
    "build": "pnpm writeCert && pnpm boot && NODE_ENV=production next build",
    "db:clean": "pnpm db:start && ./scripts/createCleanDb.ts",
    "db:import": "pnpm db:start && ./scripts/toLocal.ts",
    "db:migrate:generate": "knex migrate:make --knexfile ./support/knexfile.js",
    "db:migrate:up": "tsx node_modules/knex/bin/cli.js migrate:latest --knexfile ./support/knexfile.ts up",
    "db:resetOwner": "./scripts/resetDatabaseOwner.ts",
    "db:setup": "pnpm db:clean && pnpm db:migrate:up",
    "db:start": "docker compose up -d && ./scripts/wait-for-postgres.sh -c festive-beverage-postgres",
    "dev": "pnpm boot && next dev -p 40000 --turbo",
    "dev:debug": "pnpm boot && NODE_OPTIONS='--inspect' next dev -p 40000",
    "import": "pnpm db:clean && pnpm db:migrate:up && ./scripts/import.ts",
    "importPush": "pnpm run import && pnpm upload",
    "download": "./scripts/toLocal.ts",
    "upload": "./scripts/toAws.ts",
    "format": "prettier --no-color --loglevel warn --write '{src,api,scripts,types}/**/*.{[mc]?js,jsx,[mc]?ts,tsx,css,scss,json,graphql}'",
    "gitHash": "./scripts/getGitHash.sh",
    "lint": "eslint '{src,scripts,support,types}/**/*.{[mc]js,[mc]ts,[jt]s,tsx}'",
    "preinstall": "npx only-allow pnpm",
    "disabled:postinstall": "./scripts/buildSql.sh",
    "start": "BROWSER=none next start -p 40000",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "knip": "knip",
    "writeCert": "./scripts/writeDbCert.ts"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "3.5.0",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.4.1",
    "@prisma/client": "6.2.1",
    "@t3-oss/env-core": "^0.12.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.65.1",
    "@tanstack/react-query-devtools": "^5.65.1",
    "@trpc/client": "11.0.0-rc.666",
    "@trpc/next": "11.0.0-rc.666",
    "@trpc/react-query": "11.0.0-rc.666",
    "@trpc/server": "11.0.0-rc.666",
    "@vercel/node": "^5.0.2",
    "chalk": "^5.4.1",
    "clsx": "^2.1.1",
    "common-tags": "^1.8.2",
    "convert-units": "3.0.0-beta.7",
    "debug": "^4.4.0",
    "dequal": "^2.0.3",
    "dotenv": "^16.4.7",
    "fraction.js": "^5.2.1",
    "gray-matter": "^4.0.3",
    "hex-rgb": "^5.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jotai": "^2.11.1",
    "listr2": "^8.2.5",
    "luxon": "^3.5.0",
    "next": "^15.1.6",
    "notistack": "^3.0.2",
    "pg": "^8.13.1",
    "query-string": "^9.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-json-view": "^1.21.3",
    "react-spinners": "^0.15.0",
    "stringify-object": "^5.0.0",
    "superjson": "^2.2.2",
    "tempy": "^3.1.0",
    "ts-toolbelt": "^9.6.0",
    "tsx": "^4.19.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@ggascoigne/eslint-config": "^2.1.1",
    "@types/common-tags": "^1.8.4",
    "@types/debug": "~4.1.12",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jsdom": "~21.1.7",
    "@types/lint-staged": "~13.3.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.12.0",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/stringify-object": "^4.0.5",
    "@types/webpack-env": "^1.18.7",
    "@vitest/coverage-v8": "^3.0.4",
    "@vitest/ui": "^3.0.4",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
    "import-sort-style-module": "^6.0.0",
    "jsdom": "^26.0.0",
    "knex": "^3.1.0",
    "knex-migrate": "^1.7.4",
    "knip": "^5.43.6",
    "lint-staged": "^15.4.3",
    "node-fetch": "^3.3.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4",
    "zx": "^8.3.0"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "semi": false,
    "jsxSingleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "./scripts/noForbiddenCode.sh",
      "prettier --no-color --write",
      "eslint"
    ],
    "*.{css,scss,graphql}": [
      "prettier --no-color --write"
    ]
  },
  "browserslist": [
    ">0.5%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "packageManager": "pnpm@9.15.1"
}