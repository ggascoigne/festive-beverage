    "@mui/material": "^5.11.16",
    "@mui/system": "^5.11.16",
    "@oclif/core": "^2.8.0",
    "@pyramation/postgraphile-plugin-fulltext-filter": "^2.0.0",
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-query-devtools": "^4.29.0",
    "@vercel/node": "^2.10.3",
    "chalk": "^4.1.2",
    "clsx": "^1.2.1",
    "common-tags": "^1.8.2",
