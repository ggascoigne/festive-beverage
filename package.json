{
  "name": "festive-beverage",
  "version": "0.1.0",
  "description": "FestiveBeverage site",
  "author": "guy@wyrdrune.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ggascoigne/festive-beverage.git"
  },
  "files": [],
  "oclif": {
    "commands": "./scripts/cliCommands",
    "bin": "run",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "boot": "pnpm db:migrate:up && pnpm prepareGraphql && pnpm codegen && pnpm gitHash",
    "build": "pnpm boot && NODE_ENV=production next build",
    "codegen": "graphql-codegen --config codegen.yml",
    "db:clean": "pnpm db:start && node ./scripts/run.js createCleanDb",
    "db:import": "pnpm db:start && ./scripts/toLocal.sh",
    "db:migrate:down": "knex-migrate --cwd ./support --knexfile ./knexfile.cjs down",
    "db:migrate:generate": "knex-migrate --cwd ./support --knexfile ./knexfile.cjs generate",
    "db:migrate:up": "knex-migrate --cwd ./support --knexfile ./knexfile.cjs up",
    "db:setup": "pnpm db:clean && yarn db:migrate:up",
    "db:start": "docker-compose up -d && ./scripts/wait-for-postgres.sh -c festive-beverage-postgres",
    "dev": "pnpm boot && next dev -p 40000",
    "dev:debug": "pnpm boot && NODE_OPTIONS='--inspect' next dev -p 40000",
    "import": "pnpm db:clean && pnpm db:migrate:up && ./scripts/run.js import",
    "importPush": "pnpm run import && pnpm upload",
    "download": "./scripts/toLocal.sh",
    "upload": "./scripts/toAws.sh",
    "format": "pretty-quick",
    "format:all": "prettier --no-color --loglevel warn --write '{src,api,scripts,types}/**/*.{js,jsx,ts,tsx,css,scss,json,graphql}'",
    "gitHash": "./scripts/getGitHash.sh",
    "lint": "eslint '{src,api,scripts,shared,types}/**/*.{js,jsx,ts,tsx}' ./.eslintrc.js",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "prepareGraphql": "./scripts/run.js prepareGraphql",
    "disabled:postinstall": "./scripts/buildSql.sh",
    "start": "BROWSER=none next start -p 40000",
    "test": "next test"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "2.4.0",
    "@emotion/cache": "^11.10.7",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@graphile-contrib/pg-order-by-related": "^1.0.0",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@graphiql/plugin-explorer": "^0.1.15",
