  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "semi": false,
    "jsxSingleQuote": true
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    },
    ".js, .jsx": {
      "style": "module"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "./scripts/noForbiddenCode.sh",
      "prettier --no-color --write",
      "eslint"
    ],
    "*.{css,scss,graphql}": [
      "prettier --no-color --write"
    ]
  },
  "browserslist": [
    ">0.5%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "patchedDependencies": {
    "@eslint/eslintrc@2.0.2": "patches/@eslint__eslintrc@2.0.2.patch"
  }
}
