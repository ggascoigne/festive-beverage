    "gray-matter": "^4.0.3",
    "hex-rgb": "^5.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jotai": "^2.8.0",
    "jump.js": "^1.0.2",
    "jwt-decode": "^4.0.0",
    "luxon": "^3.4.4",
    "match-sorter": "^6.3.4",
    "next": "^14.1.4",
    "next-pwa": "^5.6.0",
    "notistack": "^3.0.1",
